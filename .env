# Threat Feed Aggregator Environment Variables
# Rename this file to .env and fill in the values

# Flask Secret Key (Generate a random string: python -c 'import secrets; print(secrets.token_hex(32))')
SECRET_KEY=change_this_to_a_very_secure_random_string

# Initial Admin Password (Used only on first run to set up the admin user)
ADMIN_PASSWORD=change_me_please

# Optional: Port (Default is 443)
PORT=443
